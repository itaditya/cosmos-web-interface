{
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "eslint-config-airbnb",
  "rules": {
    "arrow-parens": 0,
    "generator-star-spacing": 0,
    "quotes": [2, "single"],
    "no-console": 0,
    "no-unused-vars": 1,
    "eqeqeq": 2,
    "semi": 0,
    "strict": 1,
    "linebreak-style": 0,
    "quote-props": 0
  },
  "globals": {
      "angular": 1,
      "$": 1
    }
}
